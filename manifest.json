{
  "name": "Defund Wikipedia",
  "version": "1.2",
  "description": "Deletes donation notices of Wikipedia.",
  "permissions": ["activeTab"],
  "icons": { "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
        "matches": ["https://*.wikipedia.org/*"],
        "js": ["/content_script.js"],
        "all_frames": true
    }
  ],
  "manifest_version": 2
}